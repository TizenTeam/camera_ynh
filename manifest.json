{
  "name": "Camera",
  "id": "camera",
  "description": {
    "en": "Manage camera RP",
    "fr": "Reverse Proxy pour cameras"
  },
  "developer": {
    "name": "lunarok",
    "email": "cedric@wayang.fr",
    "url": "http://www.wayang.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for Camera"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for Camera"
        },
        "example": "/camera",
        "default": "/camera"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where Camera is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
